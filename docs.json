{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Predictus API",
  "colors": {
    "primary": "#0B3A4E",
    "light": "#3EC6E0",
    "dark": "#072A3F"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Introdução",
            "pages": [
              "index",
              "authentication"
            ]
          },
          {
            "group": "Dossiê Jurídico",
            "pages": [
              "dossie-juridico/overview",
              "dossie-juridico/buscar-por-cpf",
              "dossie-juridico/estrutura-dados"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
